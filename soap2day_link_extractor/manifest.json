{
    "name" : "Soap opner",
    "manifest_version" : 2,
    "version" : "0.1",
    "description" : "Simple extension to open video in another apps - only for soap 2 day",
    "content_scripts":[
        {
            "matches": [
                "<all_urls>"
            ],
            "js" : ["Content.js"]
        }
    ],
    "background":{
        "scripts" :["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}